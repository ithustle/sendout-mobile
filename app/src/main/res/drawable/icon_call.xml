<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <solid android:color="#E17C0B" />
            <padding
                android:bottom="15dp"
                android:left="15dp"
                android:right="15dp"
                android:top="15dp"/>

            <stroke
                android:width="1dp"
                android:color="#EEEEEE" />
        </shape>
    </item>
    <item android:drawable="@drawable/ic_keypad" />
</layer-list>